{
  "name": "step-build-ios",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/step-build-ios/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/step-build-ios/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/step-build-ios/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/step-build-ios",
        "tsConfig": "libs/step-build-ios/tsconfig.lib.json",
        "packageJson": "libs/step-build-ios/package.json",
        "main": "libs/step-build-ios/src/index.ts",
        "assets": ["libs/step-build-ios/*.md"]
      }
    }
  },
  "tags": ["step"]
}
