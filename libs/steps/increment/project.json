{
  "name": "steps-increment",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/steps/increment/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/steps/increment/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/steps/increment/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/steps/increment",
        "tsConfig": "libs/steps/increment/tsconfig.lib.json",
        "packageJson": "libs/steps/increment/package.json",
        "main": "libs/steps/increment/src/index.ts",
        "assets": ["libs/steps/increment/*.md"]
      }
    }
  },
  "tags": ["step"]
}
