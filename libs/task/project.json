{
  "name": "task",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/task/src",
  "projectType": "library",
  "targets": {
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "cd dist/libs/task && npm publish --registry=https://registry.npmjs.org --access public"
      }
    },
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/task",
        "tsConfig": "libs/task/tsconfig.lib.json",
        "packageJson": "libs/task/package.json",
        "main": "libs/task/src/index.ts",
        "assets": ["libs/task/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/task/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/task/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
